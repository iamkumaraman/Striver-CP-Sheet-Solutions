Link of question :- https://codeforces.com/problemset/problem/1352/A

C++ Code :-

#include <bits/stdc++.h>
using namespace std;
int main(){
    int t; cin>>t;
    while(t--){
        int n; cin>>n;
        vector<int> v;
        int p=0;
        while(n>0){
            int rem=n%10;
            if(rem!=0){
                v.push_back(rem*pow(10,p));
            }
            p++;
            n/=10;
        }
        cout<<v.size()<<endl;
        for(auto x: v){
            cout<<x<<" ";
        }
        cout<<endl;
    }
}
